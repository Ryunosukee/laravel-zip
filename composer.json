{
  "name": "dcorreah/laravel-zip",
  "description": "laravel-zip is the world's leading zip utility for file compression and backup.",
  "license": "MIT",
  "authors": [
    {
      "name": "Ryunosukee",
      "email": "info@dcorreah.net",
      "homepage": "http://www.dcorreah.net"
    }
  ],
  "keywords": [
    "laravel-zip",
    "laravel10",
    "laravel11",
    "zip",
    "unzip",
    "extract",
    "backup",
    "ZipArchive",
    "multiple",
    "merge"
  ],
  "require": {
    "php": "^8.0 || ^8.2",
    "illuminate/collections": "^11.0",
    "ext-zip": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "orchestra/testbench": "^9.1"
  },
  "autoload": {
    "psr-4": {
      "dcorreah\\Zip\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "dcorreah\\Zip\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "dcorreah\\Zip\\ZipServiceProvider"
      ],
      "aliases": {
        "Zip": "dcorreah\\Zip\\Facades\\Zip"
      }
    }
  }
}
